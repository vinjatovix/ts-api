									"});",
									"",
									"",
									"",
									"",
									"",
									"const statusCode = pm.response.code",
									"if (statusCode === 201) {",
									"    const path = pm.request.url.path",
									"    const bookId = path[path.length - 1]",
									"    pm.environment.set(\"bookId\", bookId);",
									"}",
									"",
									"",
									"",
									""
								],
								"type": "text/javascript"
							}
